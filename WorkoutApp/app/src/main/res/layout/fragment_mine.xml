<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UI.fragments.mineFragment">


    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:fillViewport="true"
      >


        <RelativeLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fff">
            <TextView
                android:id="@+id/text1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="About Me"
                android:textColor="#0C0C0C"
                android:layout_marginLeft="20dp"
                android:textSize="22dp"
                android:textStyle="bold"/>
            <LinearLayout
                android:id="@+id/layout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/text1"
                android:layout_marginLeft="20dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_profi"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="My Profile"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="145dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout1"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_zone"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Trouble Zone"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="120dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout2"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_days"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Training Days"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="120dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout3"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="15dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_level"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fitness Level"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="120dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>

            <Button
                android:id="@+id/button1"
                android:layout_width="300dp"
                android:layout_height="50dp"
                android:text="GO PREMIUM"
                android:textSize="18dp"
                android:textColor="#FFD700"
                android:background="@drawable/text_back"
                android:layout_below="@id/layout4"
                android:layout_centerInParent="true"
                android:layout_marginTop="20dp"
                />

            <TextView
                android:id="@+id/text2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="SUPPORT US"
                android:layout_below="@id/button1"
                android:textColor="#0C0C0C"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="20dp"
                android:textSize="22dp"
                android:textStyle="bold"/>

            <LinearLayout
                android:id="@+id/layout5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/text2"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_star"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rate us"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="164dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layout6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout5"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_questions"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Common questions"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="62dp"
                    app:srcCompat="@drawable/ic_arrow" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout6"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_share"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Share with friends"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="70dp"
                    app:srcCompat="@drawable/ic_arrow" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout7"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_feedback"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Feedback"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp" />

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="147dp"
                    app:srcCompat="@drawable/ic_arrow" />
            </LinearLayout>



            <LinearLayout
                android:id="@+id/layout9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout8"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_privacy"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Privacy policy"
                    android:textColor="#0C0C0C"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp" />

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="120dp"
                    app:srcCompat="@drawable/ic_arrow" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/layout10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/layout9"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp">
                <ImageView
                    android:layout_width="45dp"
                    android:layout_height="45dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/text_back"
                    app:srcCompat="@drawable/ic_log"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Log out"
                    android:textColor="#000"
                    android:textSize="18dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="25dp" />

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_weight="1"
                    android:layout_marginTop="25dp"
                    android:layout_marginLeft="157dp"
                    app:srcCompat="@drawable/ic_arrow" />
            </LinearLayout>

        </RelativeLayout>


    </ScrollView>

</FrameLayout>